{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    // "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "settings": {
    // "react": {
    //   "version": "detect"
    // }
  },
  "extends": [
    // "eslint:recommended",
    // "plugin:react/recommended",
    // "plugin:prettier/recommended",
    "plugin:react-hooks/recommended"
  ],
  "rules": {
    "no-console": "error",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
    // "simple-import-sort/imports": "error"
  }
}
